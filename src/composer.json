{
    "name": "shun57/collect-rent",
    "type": "project",
    "description": "少額取り立てサービス",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "bensampo/laravel-enum": "5.3.1",
        "guzzlehttp/guzzle": "7.4.5",
        "inertiajs/inertia-laravel": "0.5.4",
        "laravel/framework": "9.12.2",
        "laravel/sanctum": "2.15.1",
        "laravel/socialite": "5.5.2",
        "laravel/tinker": "2.7.2",
        "tightenco/ziggy": "1.4.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.6.7",
        "fakerphp/faker": "1.19.0",
        "laravel/breeze": "1.9.0",
        "laravel/sail": "1.14.4",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "6.2.0",
        "nunomaduro/larastan": "2.1.4",
        "phpunit/phpunit": "9.5.20",
        "spatie/laravel-ignition": "1.2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
